{
  "name": "@uptickart/infra",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "deploy-ui-cloudfront": "cdk deploy system-ui-cloudfront --require-approval never --app 'tsx ./src/bin/ui-cloudfront.ts'",
    "deploy-images-cloudfront": "cdk deploy system-images-cloudfront --require-approval never --app 'tsx ./src/bin/images-cloudfront.ts'",
    "deploy-services": "cdk deploy --all --require-approval never --app 'tsx ./src/bin/services.ts'",
    "deploy-rds": "cdk deploy system-rds --require-approval never --app 'tsx ./src/bin/rds.ts'",
    "destroy-ui-cloudfront": "cdk destroy system-ui-cloudfront --force --require-approval never --app 'tsx ./src/bin/ui-cloudfront.ts'",
    "destroy-images-cloudfront": "cdk destroy system-images-cloudfront --force --require-approval never --app 'tsx ./src/bin/images-cloudfront.ts'",
    "destroy-services": "cdk destroy --all --force --require-approval never --app 'tsx ./src/bin/services.ts'",
    "destroy-rds": "cdk destroy system-rds --force --require-approval never --app 'tsx ./src/bin/rds.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.234.1",
    "constructs": "^10.4.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "@aws-sdk/client-ssm": "^3.700.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.11.10",
    "aws-cdk": "^2.234.1",
    "esbuild": "^0.24.0",
    "pg": "^8.13.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
